    "class-transformer": "0.3.1",
    "class-validator": "^0.13.2",
    "cluster": "^0.7.7",
    "exception": "^1.0.9",
    "firebase-admin": "10.0.0",
    "form-data": "^4.0.0",
    "joi": "^17.9.2",
    "lodash.merge": "^4.6.2",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
