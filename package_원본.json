    "@nestjs/core": "8.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "8.0.0",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "5",
    "apple-signin-auth": "^1.7.4",
    "aws-sdk": "^2.1368.0",
    "axios": "^1.2.2",
